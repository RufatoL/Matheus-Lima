

n1 = float(input("Digite o numero"))
n2 = float(input("Digite o segundo numero"))

if n1 > n2:
    print(n1, "é maior")

elif n2 > n1:
    print(n2, "É maior ")

else:
    print("Ambos os numeros sao iguais")


# Tabuada
numero = int(input("digite um numero"))
for i in range(1, 11):
    resultado = numero * i
    print(f"{numero} x {i} = {resultado}" )
